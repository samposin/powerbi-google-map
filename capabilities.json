{
    "dataRoles": [
        {
            "displayName": "Latitude",
            "name": "lat",
            "kind": "GroupingOrMeasure"
        },
        {
            "displayName": "Longitude",
            "name": "lng",
            "kind": "GroupingOrMeasure"
        },
        {
            "displayName": "Location types",
            "name": "locationTypes",
            "kind": "Measure"
        },
        {
            "displayName": "Location Status",
            "name": "status",
            "kind": "Measure"
        },
        {
            "displayName": "Tool Tip",
            "name": "toolTip",
            "kind": "GroupingOrMeasure"
        }
    ],
    "objects": {
        "myCustomObject": {
            "displayName": "Google Map Settings",
            "properties": {
                "token": {
                    "displayName": "token",
                    "type": {
                        "text": true
                    }
                },
                "iconURL": {
                    "displayName": "Icon URL",
                    "type": {
                        "text": true
                    }
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "category": {
                        "max": 1
                    }
                }
            ],
            "table": {
                "rows": {
                    "select": [
                        {
                            "for": {
                                "in": "lat"
                            }
                        },
                        {
                            "for": {
                                "in": "lng"
                            }
                        },
                        {
                            "for": {
                                "in": "locationTypes"
                            }
                        },
                        {
                            "for": {
                                "in": "status"
                            }
                        },
                        {
                            "for": {
                                "in": "toolTip"
                            }
                        }
                    ]
                }
            }
        }
    ]
